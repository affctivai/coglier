    "version": "0.2.0",
    "configurations": [
        {
            "name": "0.0 GAMEEMO",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/0.0 gameemo preprocessing.py",
            "args": ["--window","256",
                     "--stride","128", 
                     "--method","PSD"] //DE, PSD, seg
        },
        {
            "name": "0.0 SEED-IV",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/0.0 seed_iv preprocessing.py",
            "args": [
                     "--window","400",
                     "--stride","200", 
                     "--method","seg"] //DE, PSD, seg
        },
        {
            "name": "0.1 make dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/0.1 make dataset.py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "a"]
        },
        {
            "name": "0.2 subdepend",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/0.2 subdepend.py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "a",
                     "--model", "TSC",
                     "--feature", "raw",
                     "--epoch", "130",
                     "--project_name", "Subdepend"]
        },
        {
            "name": "detector-High",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/1.0 OOD detector (High).py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "v",
                     "--model", "CCNN",
                     "--feature", "DE",
                     "--epoch", "100",
                     "--dropout", "0.5",
                    //  "--test",
                     "--column", "test_acc",
                     "--cut", "5"]
        },
        {
            "name": "analysis",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/analysis.py",
            "args": ["--dataset", "SEED",
                     "--label", "4",
                     "--model", "CCNN",
                     "--feature", "DE",
                     "--column", "test_acc",
                     "--cut", "4",
                     "--thresholds", "0.95"]
        },
        {
            "name": "2.0 Base",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/2.0 Base_Remove.py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "a",
                     "--model", "TSC",
                     "--feature", "raw",
                     "--epoch", "200",
                     "--dropout", "0.5",
                    //  "--test",
                     "--threshold", "0.80",
                     "--topk", "2"]
        },
    ]
}