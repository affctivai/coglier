    "version": "0.2.0",
    "configurations": [
        {
            "name": "0.0 GAMEEMO",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/0.0 GAMEEMO preprocessing.py",
            "args": ["--window","256",
                     "--stride","128", 
                     "--method","PSD"]
        },
        {
            "name": "0.1 make dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/0.1 make dataset.py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "v"]
        },
        {
            "name": "0.2 subdepend",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/0.2 subdepend.py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "v",
                     "--model", "CCNN",
                     "--feature", "DE",
                     "--epoch", "100",
                     "--project_name", "Subdepend"]
        },
        {
            "name": "1.0 baseline",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/1.0 baseline.py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "v",
                     "--model", "CCNN",
                     "--feature", "DE",
                     "--epoch", "110",
                     "--dropout", "0.5",
                    //  "--test",
                    //  "--base",
                     "--threshold", "0.75",
                     "--topk", "2"]
        },
        {
            "name": "2.0 High",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/2.0 High.py",
            "args": ["--dataset", "GAMEEMO",
                     "--label", "v",
                     "--model", "CCNN",
                     "--feature", "DE",
                     "--epoch", "120",
                     "--dropout", "0.5",
                     "--column", "test_acc",
                     "--cut", "6"]
        }
    ]
}